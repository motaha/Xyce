			vl0=-0.5*(dv0+mv0)
			xvar1=(1.0-vl0/PC)
			xvar2=(1.0-MC)
			xvar3=xvar1**xvar2
			q0=-PC*xvar3/(1.0-MC)
			dv=Vbep+dv0
			mv=sqrt(dv*dv+4.0*AJC*AJC)
			vl=0.5*(dv-mv)-dv0
			xvar1=(1.0-vl/PC)
			xvar2=(1.0-MC)
			xvar3=xvar1**xvar2
			qlo=-PC*xvar3/(1.0-MC)
			xvar1=(1.0-FC)
			xvar2=(-MC)
			xvar3=xvar1**xvar2
			qdbep=qlo+xvar3*(Vbep-vl+vl0)-q0
		}
	}
	argi=Vbei/(NF*Vtv)
	expi=limexp(argi)
	Ifi=IS*(expi-1.0)
	argi=Vbci/(NR*Vtv)
	expi=limexp(argi)
	Iri=IS*ISRR*(expi-1.0)
	q1z=1.0+qdbe*IVER+qdbc*IVEF
	q1=0.5*(sqrt((q1z-1.0e-4)*(q1z-1.0e-4)+1.0e-8)+q1z-1.0e-4)+1.0e-4
	q2=Ifi*IIKF+Iri*IIKR
	if (QBM<0.5) {
		xvar2=1.0/NKF
		xvar3=q1**xvar2
		xvar1=(xvar3+4.0*q2)
		xvar4=xvar1**NKF
		qb=0.5*(q1+xvar4)
	} else {
		xvar1=(1.0+4.0*q2)
		xvar2=xvar1**NKF
		qb=0.5*q1*(1.0+xvar2)
	}
	Itzr=Iri/qb
	Itzf=Ifi/qb
	Ixzf=-Itzf
	Itxf=Vrxf
	Ixxf=Vrxf
	if (ISP>0.0) {
		argi=Vbep/(NFP*Vtv)
		expi=limexp(argi)
		argx=Vbci/(NFP*Vtv)
		expx=limexp(argx)
		Ifp=ISP*(WSP*expi+(1.0-WSP)*expx-1.0)
		q2p=Ifp*IIKP
		qbp=0.5*(1.0+sqrt(1.0+4.0*q2p))
	} else {
		Ifp=0.0
		qbp=1.0
	}
	if (WBE==1.0) {
		argi=Vbei/(NEI*Vtv)
		expi=limexp(argi)
		argn=Vbei/(NEN*Vtv)
		expn=limexp(argn)
		if (VBBE>0.0) {
			argx=(-VBBE-Vbei)/(NBBE*Vtv)
			expx=limexp(argx)
			Ibe=IBEI*(expi-1.0)+IBEN*(expn-1.0)-IBBE*(expx-EBBE)
		} else {
			Ibe=IBEI*(expi-1.0)+IBEN*(expn-1.0)
		}
		Ibex=0.0
	} else if (WBE==0.0) {
		Ibe=0.0
		argi=Vbex/(NEI*Vtv)
		expi=limexp(argi)
		argn=Vbex/(NEN*Vtv)
		expn=limexp(argn)
		if (VBBE>0.0) {
			argx=(-VBBE-Vbex)/(NBBE*Vtv)
			expx=limexp(argx)
			Ibex=IBEI*(expi-1.0)+IBEN*(expn-1.0)-IBBE*(expx-EBBE)
		} else {
			Ibex=IBEI*(expi-1.0)+IBEN*(expn-1.0)
		}
	} else {
		argi=Vbei/(NEI*Vtv)
		expi=limexp(argi)
		argn=Vbei/(NEN*Vtv)
		expn=limexp(argn)
		if (VBBE>0.0) {
			argx=(-VBBE-Vbei)/(NBBE*Vtv)
			expx=limexp(argx)
			Ibe=WBE*(IBEI*(expi-1.0)+IBEN*(expn-1.0)-IBBE*(expx-EBBE))
		} else {
			Ibe=WBE*(IBEI*(expi-1.0)+IBEN*(expn-1.0))
		}
		argi=Vbex/(NEI*Vtv)
		expi=limexp(argi)
		argn=Vbex/(NEN*Vtv)
		expn=limexp(argn)
		if (VBBE>0.0) {
			argx=(-VBBE-Vbex)/(NBBE*Vtv)
			expx=limexp(argx)
			Ibex=(1.0-WBE)*(IBEI*(expi-1.0)+IBEN*(expn-1.0)-IBBE*(expx-EBBE))
		} else {
			Ibex=(1.0-WBE)*(IBEI*(expi-1.0)+IBEN*(expn-1.0))
		}
	}
	argi=Vbci/(NCI*Vtv)
	expi=limexp(argi)
	argn=Vbci/(NCN*Vtv)
	expn=limexp(argn)
	Ibcj=IBCI*(expi-1.0)+IBCN*(expn-1.0)
	if ((IBEIP>0.0)||(IBENP>0.0)) {
		argi=Vbep/(NCI*Vtv)
		expi=limexp(argi)
		argn=Vbep/(NCN*Vtv)
		expn=limexp(argn)
		Ibep=IBEIP*(expi-1.0)+IBENP*(expn-1.0)
	} else {
		Ibep=0.0
	}
	if (AVC1>0.0) {
		vl=0.5*(sqrt((PC-Vbci)*(PC-Vbci)+0.01)+(PC-Vbci))
		xvar2=(MC-1.0)
		xvar3=vl**xvar2
		xvar1=-AVC2*xvar3
		xvar4=limexp(xvar1)
		avalf=AVC1*vl*xvar4
		Igc=(Itxf-Itzr-Ibcj)*avalf
	} else {
		Igc=0.0
	}
	Ibc=Ibcj-Igc
	if (RCX>0.0) {
		Ircx=Vrcx/RCX
	} else {
		Ircx=0.0
	}
	argi=Vbci/Vtv
	expi=limexp(argi)
	argx=Vbcx/Vtv
	expx=limexp(argx)
	Kbci=sqrt(1.0+GAMM*expi)
	Kbcx=sqrt(1.0+GAMM*expx)
	if (RCI>0.0) {
		rKp1=(Kbci+1.0)/(Kbcx+1.0)
		xvar1=ln(rKp1)
		Iohm=(Vrci+Vtv*(Kbci-Kbcx-xvar1))/RCI
		derf=IVO*RCI*Iohm/(1.0+0.5*IVO*IHRCF*sqrt(Vrci*Vrci+0.01))
		Irci=Iohm/sqrt(1.0+derf*derf)
	} else {
		Irci=0.0
	}
	if (RBX>0.0) {
		Irbx=Vrbx/RBX
	} else {
		Irbx=0.0
	}
	if (RBI>0.0) {
		Irbi=Vrbi*qb/RBI
	} else {
		Irbi=0.0
	}
	if (RE>0.0) {
		Ire=Vre/RE
	} else {
		Ire=0.0
	}
	if (RBP>0.0) {
		Irbp=Vrbp*qbp/RBP
	} else {
		Irbp=0.0
	}
	if (Ifi>0.0) {
		sgIf=1.0
	} else {
		sgIf=0.0
	}
	rIf=Ifi*sgIf*IITF
	mIf=rIf/(rIf+1.0)
	xvar1=Vbci*IVTF/1.44
	xvar2=limexp(xvar1)
	tff=TF*(1.0+QTF*q1)*(1.0+XTF*xvar2*(slTF+mIf*mIf)*sgIf)
	Qbe=CJE*qdbe*WBE+tff*Ifi/qb
	Qbex=CJE*qdbex*(1.0-WBE)
	Qbc=CJC*qdbc+TR*Iri+QCO*Kbci
	Qbcx=QCO*Kbcx
	Qbep=CJEP*qdbep+TR*Ifp
	Qbeo=Vbe*CBEO
	Qbco=Vbc*CBCO
	Flxf=LEP*Ixxf
	Qcxf=CEP*Vcxf
}
equations {

#
#	Current and charge contributions
#

	i(bi->ei)+=Ibe
	i(bx->ei)+=Ibex
	i(ci->ei)+=Itxf
	i(ei->ci)+=Itzr
	i(bi->ci)+=Ibc
	i(bx->bp)+=Ibep
	i(c->cx)+=Ircx
	i(cx->ci)+=Irci
	i(b->bx)+=Irbx
	i(bx->bi)+=Irbi
	i(e->ei)+=Ire
	i(bp->cx)+=Irbp
	i(bi->ei)+=d_by_dt(Qbe)
	i(bx->ei)+=d_by_dt(Qbex)
	i(bi->ci)+=d_by_dt(Qbc)
	i(bi->cx)+=d_by_dt(Qbcx)
	i(bx->bp)+=d_by_dt(Qbep)
	i(b->e)+=d_by_dt(Qbeo)
	i(b->c)+=d_by_dt(Qbco)
	i(xf1->gnd)+=Ixzf
	i(xf2->gnd)+=Ixxf
	i(xf1->gnd)+=d_by_dt(Qcxf)
	i(xf1->xf2)+=Flxf
}
}
